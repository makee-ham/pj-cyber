{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~contexts/*": [
                "src/contexts/*"
            ],
            "~data/*": [
                "src/data/*"
            ],
            "~layouts/*": [
                "src/layouts/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~styles/*": [
                "src/styles/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}