{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~contexts/*": [
                "src/contexts/*"
            ],
            "~data/*": [
                "src/data/*"
            ],
            "~layouts/*": [
                "src/layouts/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~styles/*": [
                "src/styles/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~icons/*": [
                "src/assets/icons/*"
            ],
            "~images/*": [
                "src/assets/images/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~HomeCategoryContext/*": [
                "src/contexts/HomeCategoryContext/*"
            ],
            "~CategoryData/*": [
                "src/contexts/CategoryData/*"
            ]
        }
    }
}